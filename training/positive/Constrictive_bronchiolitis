oad.wikimedia.org/wikipedia/commons/thumb/7/7d/Diacetyl_structure.png/84px-Diacetyl_structure.png","wgULSAcceptLanguageList":[],"wgULSCurrentAutonym":"English","wgFlaggedRevsParams":{"tags":{"status":{"levels":1,"quality":2,"pristine":3}}},"wgStableRevisionId":null,"wgCategoryTreePageCategoryOptions":"{\"mode\":0,\"hideprefix\":20,\"showcount\":true,\"namespaces\":false}","wgNoticeProject":"wikipedia","wgInternalRedirectTargetUrl":"/wiki/Bronchiolitis_obliterans","wgWikibaseItemId":"Q614750"});
